<%# add navigation links to this file %>



<header data-type="settings">
  <div class="header-wrap">
    <div class="container">
      <div class="lock"></div>
      <div class="logo">
        <h1><%= link_to 'Chalkable Attendance', root_path %></h1>
      </div>
      <a href="#" class="hamburger"></a>
    </div>
    <div id="navver">
      <ul>
        <% if user_signed_in? %>
            <li><%= link_to 'My Classes', sections_path, class: 'navbar-brand' %></li>
            <li><%= link_to 'Edit account', edit_user_registration_path, class: 'navbar-brand' %></li>
            <li><%= link_to 'Sign out', destroy_user_session_path, :method=>'delete', class: 'navbar-brand' %></li>
        <% else %>
            <li><%= link_to 'Sign in', new_user_session_path %></li>
            <li><%= link_to 'Sign up', new_user_registration_path %></li>
        <% end %>
        <% if user_signed_in? %>
            <% if current_user.admin? %>
                <li><%= link_to 'Users', users_path %></li>
            <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</header>