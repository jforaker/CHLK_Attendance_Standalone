<h1>Editing stu</h1>

<%= simple_form_for @student do |f| %>
    <%= f.input :name %>
    <%= f.button :submit %>
    <%= f.input :section_id, :as => :hidden, :input_html => { :value => @student.section_id } %>
    <%= f.input :teacher_id, :as => :hidden, :input_html => { :value => current_user.id } %>
<% end %>